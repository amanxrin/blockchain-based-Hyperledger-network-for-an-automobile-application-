<h1>{{title}}</h1>

<h2>Create Car</h2>
<form id="addForm">
  <table>
    <tr>
      <td>CarId</td>
      <td><input id="carId" type="text" /></td>
    </tr>
    <tr>
      <td>Make</td>
      <td><input id="make" type="text" /></td>
    </tr>
    <tr>
      <td>Model</td>
      <td><input id="model" type="text" /></td>
    </tr>
    <tr>
      <td>Color</td>
      <td><input id="color" type="text" /></td>
    </tr>
    <tr>
      <td>Date of Manufacture</td>
      <td><input id="dateOfManufacture" type="text" /></td>
    </tr>
    <tr>
      <td>Manufacturer Name</td>
      <td><input id="manufacturerName" type="text" /></td>
    </tr>
    <tr>
      <td></td>
      <td><button onclick="addData(event)">Create Car</button>
        <button type="reset">Clear</button></td>
    </tr>
  </table>
</form>

<h2>Query Car</h2>
<form id="readForm">
  <table>
    <tr>
      <td>Enter CarId</td>
      <td>
        <input id="carIdInput" type="text" />
      </td>
    </tr>
    <tr>
      <td>
        <button type="submit" onclick="readData(event)">Query Car</button>
        <button type="reset">Clear</button>
      </td>
    </tr>
  </table>
</form>